<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <android.support.v7.widget.CardView
        android:id="@+id/web_dev"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="1dp"
        app:cardElevation="5dp"
        app:cardBackgroundColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <net.cachapa.expandablelayout.ExpandableLayout
                android:id="@+id/appbar_expandable"
                app:el_expanded="false"
                app:el_duration="500"
                app:el_parallax="100"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:id="@+id/actionbar"
                    app:cardCornerRadius="0dp"
                    app:cardElevation="2dp"
                    android:layout_marginRight="0dp"
                    android:layout_marginLeft="0dp"
                    android:layout_marginBottom="3dp"
                    app:cardBackgroundColor="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="35dp">
                        <ImageView
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="5dp"
                            android:id="@+id/toggle"
                            android:src="@drawable/ic_close_black_24dp"
                            android:layout_width="25dp"
                            android:layout_height="25dp" />
                        <TextView
                            android:textSize="15sp"
                            android:layout_marginLeft="10dp"
                            android:gravity="center"
                            android:text="Website Development"
                            android:layout_width="wrap_content"
                            android:layout_height="25dp" />
                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </net.cachapa.expandablelayout.ExpandableLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_below="@id/appbar_expandable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <net.cachapa.expandablelayout.ExpandableLayout
                android:id="@+id/image_extanded"
                android:layout_width="match_parent"
                app:el_expanded="true"
                app:el_duration="500"
                app:el_parallax="100"
                android:layout_height="wrap_content"
                >
                <ImageView
                    android:id="@+id/training_image"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bac_cover2"
                    android:layout_width="match_parent"
                    android:layout_height="150dp" />
                <TextView
                    android:textAllCaps="true"
                    android:textSize="30sp"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_gravity="center_vertical"
                    android:text="Website Development"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </net.cachapa.expandablelayout.ExpandableLayout>
            <net.cachapa.expandablelayout.ExpandableLayout
                android:id="@+id/text_extended"
                app:el_duration="500"
                app:el_parallax="100"
                android:layout_below="@id/image_extanded"
                android:layout_width="match_parent"
                app:el_expanded="false"
                android:layout_height="wrap_content"
                >
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textStyle="bold"
                        android:fontFamily="sans-serif-thin"
                        android:textColor="@color/extradarkgray"
                        android:text="@string/webdev"
                        android:textSize="15sp"
                        android:layout_margin="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                    <Button
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="1dp"
                        android:layout_marginBottom="10dp"
                        android:id="@+id/webdev_enquiry"
                        android:textSize="18sp"
                        android:textColor="@color/white"
                        android:fontFamily="sans-serif"
                        android:text="ENQUIRY"
                        android:background="@color/colorPrimary"
                        android:layout_width="300dp"
                        android:layout_height="40dp"/>
                </LinearLayout>


            </net.cachapa.expandablelayout.ExpandableLayout>
        </RelativeLayout>
    </LinearLayout>

</android.support.v7.widget.CardView>
