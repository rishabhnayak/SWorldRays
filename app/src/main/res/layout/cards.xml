                        app:cardBackgroundColor="@color/white"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <RelativeLayout
                            android:gravity="center_vertical"
                            android:layout_width="match_parent"
                            android:layout_height="35dp">
                            <ImageView
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="5dp"
                                android:id="@+id/toggle5"
                                android:src="@drawable/ic_close_black_24dp"
                                android:layout_width="25dp"
                                android:layout_height="25dp" />
                            <TextView
                                android:textSize="15sp"
                                android:layout_marginLeft="10dp"
                                android:gravity="center"
                                android:text="SEO"
                                android:layout_width="wrap_content"
                                android:layout_height="25dp" />
                        </RelativeLayout>
                    </android.support.v7.widget.CardView>
                </net.cachapa.expandablelayout.ExpandableLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_below="@id/appbar_expandable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <net.cachapa.expandablelayout.ExpandableLayout
                    android:id="@+id/image_extanded5"
                    android:layout_width="match_parent"
                    app:el_expanded="true"
                    app:el_duration="500"
                    app:el_parallax="100"
                    android:layout_height="wrap_content"
                    >
                    <ImageView
                        android:id="@+id/training_image5"
                        android:scaleType="centerCrop"
                        android:src="@drawable/seo"
                        android:layout_width="match_parent"
                        android:layout_height="150dp" />
                    <TextView
                        android:textAllCaps="true"
                        android:textSize="30sp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_gravity="center_vertical"
                        android:text="SEO"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </net.cachapa.expandablelayout.ExpandableLayout>
                <net.cachapa.expandablelayout.ExpandableLayout
                    android:id="@+id/text_extended5"
                    app:el_duration="500"
                    app:el_parallax="100"
                    android:layout_below="@id/image_extanded5"
                    android:layout_width="match_parent"
                    app:el_expanded="false"
                    android:layout_height="wrap_content"
                    >
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:textStyle="bold"
                            android:fontFamily="sans-serif-thin"
                            android:textColor="@color/extradarkgray"
                            android:text="@string/digmar"
                            android:textSize="15sp"
                            android:layout_margin="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <Button
                            android:layout_gravity="center_horizontal"
                            android:layout_marginTop="1dp"
                            android:layout_marginBottom="10dp"
                            android:id="@+id/seo_enquiry"
                            android:textSize="18sp"
                            android:textColor="@color/white"
                            android:fontFamily="sans-serif"
                            android:text="ENQUIRY"
                            android:background="@color/colorPrimary"
                            android:layout_width="300dp"
                            android:layout_height="40dp"/>
                    </LinearLayout>
                </net.cachapa.expandablelayout.ExpandableLayout>
            </RelativeLayout>
        </LinearLayout>

    </android.support.v7.widget.CardView>

</LinearLayout>